{
  "name": "alitkovsky",
  "version": "0.3.0",
  "engines": {
    "node": "22.x",
    "npm": ">=10.x"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Edge versions",
    "last 2 Firefox versions",
    "last 2 Safari versions"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "node -e \"require('fs').rmSync('.next', { recursive: true, force: true })\"",
    "build": "next build",
    "postbuild": "node scripts/strip-polyfills.js",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint .",
    "check-unused": "npx depcheck --ignores next,react,react-dom",
    "clean-unused": "npx depcheck --json | jq -r '.dependencies[]' | grep -vE '^(postcss|autoprefixer|tailwindcss|@tailwindcss/.*|next|react|react-dom|eslint.*|prettier.*)$' | xargs npm uninstall"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@next/third-parties": "^16.1.6",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.24",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.0",
    "gsap": "^3.14.2",
    "lucide-react": "^0.569.0",
    "mathjs": "^15.1.1",
    "motion": "^12.34.0",
    "next": "^16.1.6",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "svg-pathdata": "^8.0.0",
    "tailwind-merge": "^3.4.1",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.182.0",
    "vara": "^1.4.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.6",
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "9.39.2",
    "eslint-config-next": "^16.1.6"
  }
}
